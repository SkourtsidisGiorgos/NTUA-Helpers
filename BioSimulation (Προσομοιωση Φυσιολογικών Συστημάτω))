      "source": [
        "## 1 νευρώνας n εισόδων"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "_KwvNgl0p92O",
        "outputId": "e60c0c92-db6e-455f-b75a-5fc57c8560df"
      },
      "source": [
        "x = [1,0] # Είσοδος\n",
        "w = [] \n",
        "y = 0.6         # Έξοδος\n",
        "bias = 1        # πόλωση\n",
        "lr = 1          # ρυθμός μάθησης\n",
        "f = step     # Activation Function\n",
        "df = step # παράγωγος activation function\n",
        "epochs = 1\n",
        "\n",
        "\n",
        "# U should only change stuff above this line\n",
        "#----------------------------------------------------------------------#\n",
        "n = len(w)           # Αριθμός νευρώνων εισόδου\n",
        "sum = 0\n",
        "for _ in range(epochs):\n",
        "  for i in range(n):\n",
        "    sum += w[i] * x[i]\n",
        "  sum += bias\n",
        "  ypred = f(sum)\n",
        "  d = df(sum) * (y-ypred)\n",
        "  for i in range(n):\n",
        "    w[i] = w[i] + (d*x[i]*lr)\n",
        "\n",
        "  print(\"ypred\",round(ypred,3))\n",
        "  print(\"δ =\",round(d,3))\n",
        "  for i in range(len(w)):\n",
        "    print(f\"W[{i}] =\",round(w[i],3))"
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "ypred 1\n",
            "δ = -0.4\n",
            "W[0] = 0.3\n",
            "W[1] = 0.16\n",
            "W[2] = -0.26\n",
            "W[3] = -0.08\n"
          ],
          "name": "stdout"
        }
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "vAhIC2ArdjOc"
