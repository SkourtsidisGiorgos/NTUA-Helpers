        "X = [2,4,6,8]\n",
        "Y = [5,6.1,6.9,8]\n",
        "W = [1,1,1,1]\n",
        "\n",
        "# U should only change stuff above this line\n",
        "#----------------------------------------------------------------------#\n",
        "N = len(X)\n",
        "sum1=sum2=sum3=sum4=sum5=sum6=0\n",
        "for j in range(N):\n",
        "  sum1 += W[j]\n",
        "  sum2 += X[j] * W[j]\n",
        "  sum3 += W[j] * Y[j]\n",
        "  sum5 += W[j] * X[j]** 2\n",
        "  sum6 += W[j] * Y[j] * X[j]\n",
        "sum4=sum2\n",
        "B = (sum6- (sum4*sum3)/sum1) / (sum5-(sum2*sum4)/sum1)\n",
        "A = (sum3 - B*sum2)/sum1\n",
        "\n",
        "print(\"sums:\",round(sum1,4),round(sum2,4),round(sum3,4),round(sum4,4),round(sum5,4),round(sum6,4))\n",
        "print(\"A =\",round(A,4),\"\\nB =\",round(B,4))"
